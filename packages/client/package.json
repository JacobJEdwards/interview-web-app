{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss ./css/tailwind.css -o app/styles/app.css",
    "dev": "concurrently --kill-others \"remix dev\" \"npm run dev:css\"",
    "dev:css": "postcss -w ./css/tailwind.css -o app/styles/app.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "proxy": "http://localhost:6060",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server": "1.0.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "concurrently": "^8.0.1",
    "@remix-run/eslint-config": "^1.15.0",
    "@tailwindcss/typography": "^0.5.9",
    "tailwindcss": "^3.3.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "daisyui": "^2.51.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT"
}
