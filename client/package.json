{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./css/tailwind.css -o app/styles/app.css",
    "dev": "concurrently --kill-others \"remix dev\" \"npm run dev:css\"",
    "dev:css": "tailwindcss -w -i ./css/tailwind.css -o app/styles/app.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "daisyui": "^2.51.5",
    "isbot": "^3.6.8",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server": "^1.0.0",
    "tailwindcss": "^3.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "name": "client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT"
}
